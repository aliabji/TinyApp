<title>TinyApp</title>

  <% if (user_id) { %>
  <h2>User ID: <%= user_id  %></h2>

  <form action="/logout" method="POST">
    <label for="logout">Logout</label>
    <input type="submit" value="Logout">
  
  </form>
<% } else{ %>  
  <a href="/urls/login"><h4>Click here to log in!</h4></a>
  <a href="/urls/register"><h4>Don't have an account? Click here to register!</h4></a>
<% } %>




